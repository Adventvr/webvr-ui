{
  "name": "webvr-ui",
  "version": "0.9.0",
  "description": "Library that creates a UI wrapping the WebVR experience",
  "main": "src/index.js",
  "authors": [
    "Kyle Phillips <kylephillips@google.com>",
    "Jonas Jongejan <jongejan@google.com>"
  ],
  "scripts": {
    "build": "browserify src/index.js --standalone webvrui | derequire > build/webvr-ui.js",
    "start": "budo src/index.js:build/webvr-ui.js --live --verbose --port 3000 --host localhost -- --standalone webvrui",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "eventemitter3": "^2.0.2",
    "screenfull": "^3.0.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "budo": "^9.2.1",
    "derequire": "^2.0.3",
    "tape": "^4.6.2"
  },
  "keywords": [
    "vr",
    "webvr"
  ],
  "license": "Apache-2.0",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  }
}
